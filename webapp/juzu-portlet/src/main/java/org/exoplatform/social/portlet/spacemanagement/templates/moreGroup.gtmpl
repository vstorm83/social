#{param name=moreRestrictedGroups/}

<% if (moreRestrictedGroups. size() > 0) { %>
     <ul class="RestrictGroups">
       <% for (String group : moreRestrictedGroups) { %>
            <li class="alert"><span><%= group %></span><a href="javascript:void(0)" class="close"></a></li>
       <% } %>
     </ul>
<% } %>